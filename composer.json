{
    "name": "qikker-online/laravel-samlidp",
    "description": "Make your Laravel application an Idenification Provider using SAML 2.0.",
    "keywords": ["laravel", "saml", "saml 2.0", "auth", "acl", "sso", "idp"],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "^5.6|^6.0|^7.0",
        "illuminate/routing": "^5.6|^6.0|^7.0",
        "4Labs/lightSAML": "^2.0.0"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/4Labs/lightSAML"
        }
    ],
    "autoload": {
        "psr-4": {
            "CodeGreenCreative\\SamlIdp\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        },
        "laravel": {
            "providers": [
                "CodeGreenCreative\\SamlIdp\\LaravelSamlIdpServiceProvider"
            ]
        }
    }
}
